<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="estate_property_offers_form" model="ir.ui.view">
        <field name="name">estate.property.offers.form</field>
        <field name="model">estate.property.offers</field>
        <field name="arch" type="xml">
            <form string="Offers">
                <group>
                    <field name="price"/>
                    <field name="status"/>
                    <field name="partner_id"/>
                </group>
            </form>
        </field>
    </record>

    <record id="estate_property_offers_list" model="ir.ui.view">
        <field name="name">estate.property.offers.list</field>
        <field name="model">estate.property.offers</field>
        <field name="arch" type="xml">
            <tree string="Offers">
                <field name="price"/>
                <field name="status"/>
                <field name="partner_id"/>
            </tree>
        </field>
    </record>
</odoo>